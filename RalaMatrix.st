Object subclass: #MatrizRala	instanceVariableNames: 'fila columna datos rango'	classVariableNames: ''	package: 'Guia2Ej6'!!MatrizRala methodsFor: 'as yet unclassified' stamp: 'GANDY 12/5/2022 17:24'!fueraDeRango: unaPosicion|ncolumna nfila|ncolumna := unaPosicion x.nfila:= unaPosicion y.^(ncolumna > 1)|(ncolumna > rango )|(nfila<1)|(nfila < rango).! !!MatrizRala methodsFor: 'as yet unclassified' stamp: 'GANDY 12/5/2022 23:15'!nfilas: nfila ncolumnas:ncolmunafila:=nfila.columna:=ncolmuna .! !!MatrizRala methodsFor: 'initialization' stamp: 'GANDY 12/5/2022 23:15'!initialize datos:= Dictionary new.! !!MatrizRala methodsFor: 'accessing' stamp: 'GANDY 12/5/2022 23:07'!atFila: nfila columna: ncolumna put: valor(self fueraDeRango: (ncolumna @ nfila))ifTrue: [ self error: 'fuera de rango'].(valor = 0)ifTrue: [ 	((self atFila: nfila columna: ncolumna) -= 0)ifTrue:[datos removeKey: (nfila@ncolumna)] 	 ]	ifFalse: [datos at:(nfila@ncolumna) put: valor].^valor.   ! !!MatrizRala methodsFor: 'accessing' stamp: 'GANDY 12/5/2022 23:07'!atFila: nfila columna:ncolumna(self fueraDeRango: (nfila@ncolumna))ifTrue:[self error: 'fuera de rango'].^datos at:(nfila@ncolumna) ifAbsent: [ 0 ].! !!MatrizRala methodsFor: 'comparing' stamp: 'GANDY 12/5/2022 23:19'!= matriz1 to: fila do:[:i| 	1 to: columna do:[:j|		(self atFila:i columna: j) = (matriz atFila: i columna:j )ifFalse: [ ^false].		].	].^true.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!MatrizRala class	instanceVariableNames: ''!!MatrizRala class methodsFor: 'instance creation' stamp: 'GANDY 12/5/2022 23:13'!newFila: nFilas columna: nColumnas|a|a :=super new.a nfilas: nFilas ncolumnas: nColumnas .^a.! !